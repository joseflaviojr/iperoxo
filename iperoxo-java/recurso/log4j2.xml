<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN" name="com.joseflavio.iperoxo">
	<Appenders>
		<File name="File" fileName="log/iperoxo.log">
			<PatternLayout pattern="%d{yyyy-MM-dd HH:mm:ss} %-5level %logger{36} - %msg%n" />
		</File>
	</Appenders>
	<Loggers>
		<Root level="TRACE">
			<AppenderRef ref="File" />
		</Root>
	</Loggers>
</Configuration>